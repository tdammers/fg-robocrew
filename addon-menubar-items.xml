<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <meta>
        <file-type type="string">FlightGear add-on menu bar items</file-type>
        <format-version type="int">1</format-version>
    </meta>
    <menubar-items>
        <menu>
            <label>RoboCrew</label>
            <enabled type="bool">true</enabled>
            <item>
                <label>Reload Addon</label>
                <binding>
                    <command>addon-reload</command>
                    <id>nl.tobiasdammers.robocrew</id>
                </binding>
            </item>
            <item>
                <enabled type="bool">true</enabled>
                <label>Crew Status</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>robocrew-crew-status-dialog</dialog-name>
                </binding>
            </item>
        </menu>
    </menubar-items>
</PropertyList>
